{
  "presets": ["next/babel"],
  "plugins": [
    [
      "styled-components",
      { "ssr": true, "displayName": true, "preprocess": false }
    ],
    [
      "module-resolver",
      {
        "root": ["."],
        "alias": {
          "assets": "./assets",
          "components": "./components",
          "pages": "./pages",
          "static": "./static",
          "styles": "./styles",
          "lib": "./lib"
        }
      }
    ]
  ]
}
